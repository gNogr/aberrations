function c_print(text, y, c)
  y = y or 60
  if c==nil then
    print(text, 64-#tostr(text)*2-1, y)
  else
    print(text, 64-#tostr(text)*2-1, y, c)
  end
end

function cx_print(text, x, y, c)
  x = x or 64
  y = y or 60
  if c==nil then
    print(text, x-#tostr(text)*2-1, y)
  else
    print(text, x-#tostr(text)*2-1, y, c)
  end
end

function c_multi_print(text, y, lim, c)
 y = y or 60
 lim = lim or 128
 t = split(text," ")
 val = ""
 l = 0
 for s in all(t) do
  if #val*4+#s*4>lim then
   c_print(val, y+l, c)
   val = ""
   l = l+6
  end
  val=val.." "..s
 end
 c_print(val, y+l, c)
end

