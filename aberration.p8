function snum_print(val, x, y, c)
 sp=0
 if val>=10 then
 	a=flr(val/10)
 	sspr(8+a*4,0,3,4,x,y)
 	sp=sp+4
 end
 b=val%10
 sspr(8+b*4,0,3,4,x+sp,y)
end

